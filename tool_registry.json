{
  "tools": {
    "natural_language_to_powershell": {
      "name": "natural_language_to_powershell",
      "description": "Convert natural language descriptions to PowerShell commands using local AI models",
      "category": "natural_language",
      "version": "1.0.0",
      "status": "available",
      "request_schema": {
        "description": "Schema for natural language processing tool requests",
        "properties": {
          "input_text": {
            "description": "Natural language input to convert to PowerShell commands",
            "maxLength": 10000,
            "minLength": 1,
            "title": "Input Text",
            "type": "string"
          },
          "session_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Session identifier for context tracking",
            "title": "Session Id"
          },
          "context": {
            "anyOf": [
              {
                "additionalProperties": true,
                "type": "object"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Additional context information",
            "title": "Context"
          },
          "include_explanation": {
            "default": true,
            "description": "Whether to include explanation of the generated command",
            "title": "Include Explanation",
            "type": "boolean"
          },
          "include_alternatives": {
            "default": true,
            "description": "Whether to include alternative command suggestions",
            "title": "Include Alternatives",
            "type": "boolean"
          },
          "confidence_threshold": {
            "default": 0.5,
            "description": "Minimum confidence score for command suggestions",
            "maximum": 1.0,
            "minimum": 0.0,
            "title": "Confidence Threshold",
            "type": "number"
          }
        },
        "required": [
          "input_text"
        ],
        "title": "NaturalLanguageToolRequest",
        "type": "object"
      },
      "response_schema": {
        "$defs": {
          "ToolError": {
            "description": "Schema for tool error responses",
            "properties": {
              "error_code": {
                "description": "Error code identifier",
                "title": "Error Code",
                "type": "string"
              },
              "error_message": {
                "description": "Human-readable error message",
                "title": "Error Message",
                "type": "string"
              },
              "error_details": {
                "anyOf": [
                  {
                    "additionalProperties": true,
                    "type": "object"
                  },
                  {
                    "type": "null"
                  }
                ],
                "default": null,
                "description": "Additional error details",
                "title": "Error Details"
              },
              "correlation_id": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ],
                "default": null,
                "description": "Request correlation ID",
                "title": "Correlation Id"
              },
              "timestamp": {
                "description": "Error timestamp",
                "title": "Timestamp",
                "type": "string"
              },
              "recoverable": {
                "default": false,
                "description": "Whether the error is recoverable",
                "title": "Recoverable",
                "type": "boolean"
              },
              "suggested_actions": {
                "description": "Suggested recovery actions",
                "items": {
                  "type": "string"
                },
                "title": "Suggested Actions",
                "type": "array"
              }
            },
            "required": [
              "error_code",
              "error_message",
              "timestamp"
            ],
            "title": "ToolError",
            "type": "object"
          }
        },
        "description": "Schema for natural language processing tool responses",
        "properties": {
          "success": {
            "description": "Whether the request was successful",
            "title": "Success",
            "type": "boolean"
          },
          "original_input": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Original natural language input",
            "title": "Original Input"
          },
          "generated_command": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Generated PowerShell command",
            "title": "Generated Command"
          },
          "confidence_score": {
            "anyOf": [
              {
                "maximum": 1.0,
                "minimum": 0.0,
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Confidence score",
            "title": "Confidence Score"
          },
          "explanation": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Explanation of the generated command",
            "title": "Explanation"
          },
          "alternatives": {
            "description": "Alternative command suggestions",
            "items": {
              "type": "string"
            },
            "title": "Alternatives",
            "type": "array"
          },
          "session_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Session identifier",
            "title": "Session Id"
          },
          "correlation_id": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Request correlation ID",
            "title": "Correlation Id"
          },
          "processing_time_ms": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Processing time in milliseconds",
            "title": "Processing Time Ms"
          },
          "error": {
            "anyOf": [
              {
                "$ref": "#/$defs/ToolError"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "description": "Error information if unsuccessful"
          },
          "warnings": {
            "description": "Warning messages",
            "items": {
              "type": "string"
            },
            "title": "Warnings",
            "type": "array"
          }
        },
        "required": [
          "success"
        ],
        "title": "NaturalLanguageToolResponse",
        "type": "object"
      },
      "parameters": {
        "max_input_length": 10000,
        "min_confidence_threshold": 0.1,
        "max_alternatives": 5
      },
      "examples": [
        {
          "description": "List running processes",
          "request": {
            "input_text": "show me all running processes",
            "include_explanation": true
          },
          "expected_response": {
            "generated_command": "Get-Process",
            "explanation": "Lists all currently running processes"
          }
        },
        {
          "description": "Find files by extension",
          "request": {
            "input_text": "find all .txt files in current directory",
            "include_alternatives": true
          },
          "expected_response": {
            "generated_command": "Get-ChildItem -Filter '*.txt'",
            "alternatives": [
              "ls *.txt",
              "dir *.txt"
            ]
          }
        }
      ],
      "requirements": [
        "AI engine available",
        "Local AI model loaded"
      ],
      "permissions": [
        "read_system_info"
      ],
      "rate_limit": {
        "requests_per_minute": 60,
        "burst_limit": 10
      }
    },
    "execute_powershell_command": {
