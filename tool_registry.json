        },
        {
          "description": "Get system information",
          "request": {
            "command": "Get-ComputerInfo",
            "output_format": "JSON",
            "use_sandbox": false
          },
          "expected_response": {
            "success": true,
            "return_code": 0,
            "output_format": "JSON"
          }
        }
