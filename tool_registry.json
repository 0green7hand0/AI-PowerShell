      "parameters": {
        "max_execution_time": 3600,
        "max_output_size": 10485760,
        "default_timeout": 60,
        "sandbox_enabled": true
      },
      "examples": [
        {
          "description": "List directory contents",
          "request": {
            "command": "Get-ChildItem",
            "use_sandbox": true,
            "timeout": 30
